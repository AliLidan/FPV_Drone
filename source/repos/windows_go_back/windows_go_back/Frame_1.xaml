<Page x:Class="windows_go_back.Frame_1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:windows_go_back"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Frame_1">

    <DockPanel>
        <Frame x:Name="_mainFrame" Background="Yellow"  />
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Image   Grid.Column="0"  Source="maxresdefault.jpg"/>
            <Label   Grid.Column="1" Content="Это вторая форма!" Height="75" VerticalAlignment="Top" Width="469" FontSize="24"/>
            <Button  Grid.Column="2" Content="back" Height="114" VerticalAlignment="Center"  Click="backing_0"/>
        </Grid>
    </DockPanel>
</Page>
